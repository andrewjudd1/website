        }

        .resume-header {
            text-align: start;
            margin-bottom: 10px;
        }

        .resume-h1 {
            font-size: 36px;
            margin: 0;
            padding: 0;
        }

        .resume-h2 {
            font-size: 24px;
            margin: 0;
            padding: 0;
        }

        .resume-h3 {
            font-size: 18px;
            margin: 0;
            padding: 0;
        }

        .resume-p {
            margin: 0;
            padding: 0;
        }

        .section {
            margin-bottom: 30px;
        }

        .education,
        .experience,
        .skills,
        .projects {
            margin-bottom: 40px;
        }

        .resume-ul {
            margin: 5px 0;
            padding-left: 20px;
            list-style-type: square;
        }

        .resume-footer {
            text-align: center;
            color: #888;
            margin-top: 30px;
        }

        .resume-hr {
            border: 0;
            height: 1px;
            background-color: #ddd;
            margin: 20px 0;
        }

        .resume-link {
            text-decoration: none;
            color: #0056b3;
            color: hsl(0, 68%, 45%);
            font-weight: 600;
            transition: color 0.3s ease;
        }

        .resume-link:hover {
            color: hsl(0, 66%, 36%);
        }

        .card_shadow {
            box-shadow: rgba(0, 0, 0, 0.09) 0 2px 1px, rgba(0, 0, 0, 0.09) 0 4px 2px, rgba(0, 0, 0, 0.09) 0 8px 4px, rgba(0, 0, 0, 0.09) 0 16px 8px, rgba(0, 0, 0, 0.09) 0 32px 16px;
        }

        .timeline {
            position: relative;
            display: flex;
            align-items: center;
            min-width: 50px;
            /* background-color: hsl(0, 0%, 0%); */
        }

        .line {
            position: absolute;
            border-radius: 10px;
            height: 100%;
            width: 10px;
            left: 20;
        }

        .circle {
            z-index: 10;
            position: absolute;
            display: flex;
            justify-content: center;
            align-items: center;
            background-color: hsl(0, 0%, 70%);
            width: 25px;
            height: 25px;
            left: -8;
            color: white;
            border-radius: 50%;
        }

        .links-container {
            display: flex;
            justify-content: space-between;
            align-items: start;
            gap: 10px;
            padding: 0 50px;
        }

        .content-container {
            display: flex;
            flex-direction: row;
        }

        .content-display-container {
            display: flex;
            flex-direction: column;
            justify-content: space-around;
            margin: 0;
            min-width: 450px;
            max-width: 500px;
        }

        .right-side-container {
            margin: 25px 0 0 30px
        }

        .right-side-box {
            max-width: 500px;
        }

        .sample-projects-container {
            width: 100%;
            border-top: 1px solid lightgray;
            margin-top: 20px;
            padding-top: 20px;
            display: flex;
            justify-content: space-around;
        }

        @media only screen and(max-width: 805px) {
            .links-container {
                flex-direction: column;
                padding: 0;
            }

            .timeline {
                display: none;
            }

            .content-container {
                display: flex;
                flex-direction: column;
            }

            .content-display-container {
                min-width: 0;
            }

            .right-side-container {
                margin: 25px 0 0
            }

            .sample-projects-container {
                flex-direction: column;
            }
        }
    </style>
</head>

