    <script>
        function run_resume() {
            const get_el = (el) => {
                return document.getElementById(el) || null
            }
            const make_el = (el) => {
                return document.createElement(el)
            }
            const start_date = new Date()
            const start_year = start_date.getFullYear()?.toString().slice(-2)
            const years = 5
            const timeline = 365.25 * years
            const timeline_moments = [
                {
                    label: start_year - 0,
                    color: 'black',
                    line: 0,
                    background: 'hsl(0, 0%, 70%)',
                    datapoints: Array.from(
                        {
                            length: years
                        }, (_, index_point) => ({
                            position: {
                                start: index_point === 0
                                    ? '-2%'
                                    : `${(index_point) * 24.3
                                    }%`,
                                height: '0px'
                            },
                            label: start_year - years + index_point + 2
                        })),
                    position: {
                        start: '0',
                        height: '100%'
                    }
                }, {
                    line: 1,
                    background: 'hsl(100, 60%, 60%)',
                    // position: { start: '0', height: 30 },
                    datapoints: [],
                    timeline: {
                        start_date: new Date('01-JUL-21'),
                        end_date: new Date('07-MAR-22'),
                        add_position: true
                    }
                }, {
                    line: 2,
                    background: 'hsl(190, 60%, 60%)',
                    // position: { start: '100', height: 30 },
                    datapoints: [],
                    timeline: {
                        start_date: new Date('07-MAR-22'),
                        end_date: new Date()
                    }
                }, {
                    line: 3,
                    background: 'hsl(280, 60%, 60%)',
                    // position: { start: '200', height: 30 },
                    datapoints: [],
                    left: 10,
                    timeline: {
                        start_date: new Date('01-JAN-23'),
                        end_date: new Date()
                    }
                }
            ]
            const run_timeline = () => {
                const timeline_container = get_el('timeline')
                const timeline_length = timeline_container.offsetHeight
                const month_length = timeline_length / ((years - 1) * 12)
                timeline_moments.map(item => {
                    const line_el = make_el('div')
                    line_el.classList.add('line')
                    let diff_mths = 0
                    let diff_start_mths = 0
                    if (item.timeline) {
                        diff_start_mths = (item
                            .timeline
                            .start_date
                            .valueOf() - (new Date('01-JAN-21')).valueOf()) / 1000 / 2_629_800
                        diff_mths = (item
                            .timeline
                            .end_date
                            .valueOf() - item
                                .timeline
                                .start_date
                                .valueOf()) / 1000 / 2_629_800
                    }
                    line_el.style.top = item?.position?.start || `${diff_start_mths * month_length
                        }px`
                    line_el.style.height = item?.position?.height || `${diff_mths * month_length
                        }px`
                    line_el.style.backgroundColor = item
                        .background
                    if (item.left) {
                        line_el.style.width = '6px'
                        line_el.style.marginLeft = '4px'
                    }
                    timeline_container
                        .appendChild(line_el)
                    item
                        .datapoints
                        .map(point => {
                            const length = line_el.offsetHeight
                            const circle_el = make_el('div')
                            circle_el.style.top = point.position.start
                            const text_el = make_el('p')
                            text_el.innerText = point
                                .label
                            circle_el
                                .appendChild(text_el)
                            circle_el
                                .classList
                                .add('circle')
                            line_el
                                .appendChild(circle_el)
                        })
                })
            }
            run_timeline()
            function check_timeline() {
                if (get_el('timeline')) {
                    run_timeline()
                }
            }
            window.addEventListener('resize', check_timeline);
            const time_driggs_title = document.getElementById('time_driggs_title')
            const currentDate = new Date();
            const months = [
                "January",
                "February",
                "March",
                "April",
                "May",
                "June",
                "July",
                "August",
                "September",
                "October",
                "November",
                "December"
            ];
            const month = months[currentDate.getMonth()];
            const day = currentDate.getDate();
            const year = currentDate.getFullYear();
            function getDayWithSuffix(day) {
                if (day >= 11 && day <= 13) {
                    return day + "<sup>th</sup>";
                }
                switch (day % 10) {
                    case 1:
                        return day + "<sup>st</sup>";
                    case 2:
                        return day + "<sup>nd</sup>";
                    case 3:
                        return day + "<sup>rd</sup>";
                    default:
                        return day + "<sup>th</sup>";
                }
            }
            const formattedDate = `${month} ${getDayWithSuffix(day)
                }, ${year}`;
            time_driggs_title.innerHTML = 'Present'
            // `${formattedDate}`
        } run_resume()
    </script>
