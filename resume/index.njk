            }

            .resume-nav {
                display: none;
            }
        }

        .container {
            max-width: 900px;
            max-height: 1056px;
            margin: 0 auto;
            /* padding: 20px; */
            border-radius: 5px;
            /* box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1); */
